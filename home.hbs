    <style>
 
 body{
    background: url('11.jpg');
    background-size: cover;
    
    /* background-position: center; */
 }
        .top-bar {
            background-color: #0d6efd;
            color: white;
            padding: 8px 0;
            font-size: 14px;
        }

        .top-bar i {
            margin-right: 5px;
        }

    
        .navbar {
            padding: 15px 0;
            background-color: white;
            box-shadow: 0 2px 15px rgba(0, 0, 0, 0.1);
        }

        .navbar-brand {
            font-size: 24px;
            font-weight: bold;
            color: #000000 !important;
        }

        .nav-link {
            color: #333 !important;
            font-weight: 500;
            margin: 0 10px;
            padding: 8px 0;
            position: relative;
            transition: color 0.3s ease;
        }

        .nav-link:hover {
            color: #0d6efd !important;
        }

        .nav-link::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            width: 100%;
            height: 2px;
            background-color: #0d6efd;
            transform: scaleX(0);
            transition: transform 0.3s ease;
        }

        .nav-link:hover::after {
            transform: scaleX(1);
        }

   
        .upload-link{
            background: linear-gradient(135deg, #79808c, #070809);
            color: white;
            border: none;
            padding: 8px 20px;
            border-radius: 5px;
            transition: all 0.3s ease;
            font-size: large;
            font-weight: 500;  
        }

.upload-link:hover {
    background: linear-gradient(135deg, #09ccbc, #007acc);
    
            transform: translateY(-1px);
            box-shadow: 0 4px 15px rgba(13, 110, 253, 0.2);
}

p{
    position: absolute;
    justify-content: center;
    align-items: center;
    font-style: italic;
    padding: 100px;
    margin-left: 85px;
    margin-right: 100px;
    /* height: 100px; */
    font-weight: 500;
    font-size: 60px;
    color: rgb(10, 81, 235);
    text-shadow: 5px 5px 5px  #93b9b9;
}


        @media (max-width: 991px) {
            .top-bar {
                display: none;
            }

            .navbar-collapse {
                background-color: white;
                padding: 15px;
                border-radius: 8px;
                margin-top: 10px;
                box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
            }

            .nav-link {
                margin: 5px 0;
                padding: 8px 15px;
            }

            .nav-link::after {
                display: none;
            }

            .upload-btn {
                width: 100%;
                margin-top: 10px;
            }
        }



        @keyframes fadeIn {
    from {
        opacity: 0;
        transform: translateY(20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

/* Logo Animation */
.navbar-brand img {
    animation: fadeIn 1s ease-out;
    animation-fill-mode: backwards;
}

/* Navigation Links Animation */
@keyframes slideInFromLeft {
    from {
        opacity: 0;
        transform: translateX(-50px);
    }
    to {
        opacity: 1;
        transform: translateX(0);
    }
}

.navbar-nav .nav-item {
    opacity: 0;
    animation: slideInFromLeft 0.7s forwards;
}

.navbar-nav .nav-item:nth-child(1) { animation-delay: 0.2s; }
.navbar-nav .nav-item:nth-child(2) { animation-delay: 0.4s; }
.navbar-nav .nav-item:nth-child(3) { animation-delay: 0.6s; }
.navbar-nav .nav-item:nth-child(4) { animation-delay: 0.8s; }

/* Upload Button Animation */
@keyframes buttonGlow {
    0%, 100% {
        box-shadow: 0 0 5px rgba(13, 110, 253, 0.3);
    }
    50% {
        box-shadow: 0 0 20px rgba(13, 110, 253, 0.7);
    }
}

.upload-link {
    animation: 
        fadeIn 1s ease-out,
        buttonGlow 2s infinite;
    animation-delay: 1s;
    animation-fill-mode: backwards;
}

/* Tagline Animation */
@keyframes textReveal {
    from {
        opacity: 0;
        clip-path: polygon(0 0, 0 0, 0 100%, 0% 100%);
    }
    to {
        opacity: 1;
        clip-path: polygon(0 0, 100% 0, 100% 100%, 0 100%);
    }
}

p {
    animation: 
        textReveal 1.5s cubic-bezier(0.7, 0, 0.3, 1) forwards,
        fadeIn 1.5s ease-out;
    animation-delay: 1s;
    animation-fill-mode: backwards;
}

/* Background Subtle Animation */
@keyframes backgroundWave {
    0% { background-position: 0% 50%; }
    50% { background-position: 100% 50%; }
    100% { background-position: 0% 50%; }
}

/* Hover Effects */
.nav-link {
    transition: all 0.3s ease;
}

.nav-link:hover {
    transform: scale(1.05);
    color: #0b53fa !important;
}

/* Responsive Animations */
@media (max-width: 991px) {
    .navbar-nav .nav-item {
        animation: fadeIn 0.9s forwards;
        opacity: 0;
    }
    
    .navbar-nav .nav-item:nth-child(1) { animation-delay: 0.1s; }
    .navbar-nav .nav-item:nth-child(2) { animation-delay: 0.2s; }
    .navbar-nav .nav-item:nth-child(3) { animation-delay: 0.3s; }
    .navbar-nav .nav-item:nth-child(4) { animation-delay: 0.4s; }
}

@keyframes backgroundMovement {
    0% {
        background-position: 0% 50%;
    }
    50% {
        background-position: 100% 50%;
    }
    100% {
        background-position: 0% 50%;
    }
}

@keyframes backgroundColorShift {
    0% {
        filter: brightness(100%);
    }
    50% {
        filter: brightness(105%);
    }
    100% {
        filter: brightness(100%);
    }
}

body {
    background: url('11.jpg');
    background-size: cover;
    background-position: center;
    background-attachment: fixed;
    animation: 
        backgroundMovement 20s linear infinite,
        backgroundColorShift 15s ease-in-out infinite;
    transition: background-position 1s ease;
}

/* Soft Overlay Effect */
body::before {
    content: "";
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(
        45deg, 
        rgba(13, 110, 253, 0.05), 
        rgba(13, 110, 253, 0.02)
    );
    z-index: -1;
}

/* Responsive Adjustments */
@media (max-width: 768px) {
    body {
        animation: 
            backgroundMovement 25s linear infinite;
    }
}






.image-animate {
    transition: all 0.4s ease-in-out;
    transform-origin: center;
}

/* Zoom on Hover */
.image-zoom:hover {
    transform: scale(1.05);
    box-shadow: 0 10px 20px rgba(0,0,0,0.1);
}

/* Subtle Rotate on Hover */
.image-rotate:hover {
    transform: rotate(2deg) scale(1.02);
}

/* Pulse Animation */
@keyframes pulse {
    0% {
        transform: scale(1);
    }
    50% {
        transform: scale(1.05);
    }
    100% {
        transform: scale(1);
    }
}

.image-pulse {
    animation: pulse 2s ease-in-out infinite;
}

/* Fade In Animation */
@keyframes fadeInImage {
    from {
        opacity: 0;
        transform: translateY(20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

.image-fadein {
    opacity: 0;
    animation: fadeInImage 1s ease-out forwards;
}

/* Parallax-like Effect */
.image-parallax {
    transition: transform 0.5s ease;
}

.image-parallax:hover {
    transform: translateY(-10px);
}

/* Glitch Effect */
@keyframes glitch {
    2%, 64% {
        transform: translate(2px, 0) skew(0deg);
    }
    4%, 60% {
        transform: translate(-2px, 0) skew(0deg);
    }
    62% {
        transform: translate(0, 0) skew(5deg);
    }
}

.image-glitch:hover {
    animation: glitch 1s linear infinite;
}

/* Border Reveal */
@keyframes borderReveal {
    from {
        clip-path: inset(0 100% 0 0);
    }
    to {
        clip-path: inset(0);
    }
}

.image-border-reveal {
    position: relative;
    overflow: hidden;
}

.image-border-reveal::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border: 3px solid #0d6efd;
    pointer-events: none;
    animation: borderReveal 1s forwards;
}

/* Usage Classes */
.navbar-brand img {
    transition: transform 0.3s ease;
}

.navbar-brand img:hover {
    transform: scale(1.1) rotate(5deg);
}

/* Responsive Adjustments */
@media (max-width: 768px) {
    .image-zoom:hover,
    .image-rotate:hover {
        transform: none;
    }
}
